# Function
snippet fn
	function ${1:function_name}(${2:argument}) {
		${3:// body...}
	}
# for (...) {...} (Improved Native For-Loop)
snippet fori
	for (var ${2:i} = ${1:Things}.length - 1; $2 >= 0; $2${3:--}) {
		${4:$1[$2]}
	}
snippet if
	if(${1})
	{
		${2}
	}
snippet ife
	if(${1})
	{
		${2}
	}
	else
	{
		${3}
	}
snippet else
	else
	{
		${1}
	}
snippet elif
	elseif(${1})
	{
		${2}
	}
